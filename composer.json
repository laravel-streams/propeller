{
    "name": "streams/propeller",
    "description": "Minimal Laravel Streams authentication scaffolding with Blade and Tailwind.",
    "keywords": ["laravel", "streams", "auth"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel-streams/propeller/issues",
        "source": "https://github.com/laravel-streams/propeller"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Ryan Thompson",
            "email": "ryan@streams.dev"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/filesystem": "^8.42",
        "illuminate/support": "^8.42",
        "illuminate/validation": "^8.42"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Breeze\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Breeze\\BreezeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
